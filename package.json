{
  "name": "luxorpool",
  "description": "Unofficial Luxor Mining Pool API Wrapper",
  "version": "1.0.3",
  "author": "Scott Pickering",
  "bugs": "https://github.com/scottypickering/luxorpool/issues",
  "dependencies": {
    "big.js": "^6.2.1",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/big.js": "^6.1.6",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./build/cjs/index.d.ts",
        "default": "./build/cjs/index.js"
      },
      "import": {
        "types": "./build/esm/index.d.ts",
        "default": "./build/esm/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://github.com/scottypickering/luxorpool",
  "keywords": [
    "luxor",
    "bitcoin",
    "mining",
    "pool"
  ],
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/scottypickering/luxorpool.git"
  },
  "scripts": {
    "dev": "rm -rf dist && tsc --watch",
    "prebuild": "rimraf dist",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags"
  },
  "type": "module",
  "types": "dist/esm/index.d.ts"
}
